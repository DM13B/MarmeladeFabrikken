<Window x:Class="MarmeladeFabrikken.AddProduct"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       Title="AddProduct" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="ProductTextBox" HorizontalAlignment="Left" Height="23" Margin="10,11,0,0" TextWrapping="Wrap" Text="Produkt" VerticalAlignment="Top" Width="120"/>
        <ListView x:Name="ControlListview" HorizontalAlignment="Left" Height="194" Margin="10,66,0,0" VerticalAlignment="Top" Width="272">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="ButtonSaveProduct" Content="Gem Produkt" HorizontalAlignment="Left" Margin="207,38,0,0" VerticalAlignment="Top" Width="75" Click="ButtonSaveProduct_Click"/>
        <Button x:Name="ButtonNewControl" Content="Tilføj Kontrol" HorizontalAlignment="Left" Margin="32,39,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
